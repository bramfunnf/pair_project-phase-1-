<!DOCTYPE html>
<html lang="en">
<%- include('./partial/head.ejs') %>
<body>
    <div>
        <form action=<%= `/schedule/edit/${data[0].SubjectId}` %> method="POST"> 
            <label>Date</label><br>
            <input type="date" name="date" value= <%= data[0].date.toISOString().substring(0, 10) %>><br><br>
            <label>link</label><br>
            <input type="text" name="link" value= <%= data[0].link %>><br><br>
            <label>Student Limit</label><br>
            <input type="text" name="student_limit" value= <%= data[0].student_limit %>><br><br>
            <input type="submit" value="Submit">
        </form>
        <a href="/schedule/teacher">
            <input type="submit" value="Cancel">
        </a>
    </div>
</body>
</html>